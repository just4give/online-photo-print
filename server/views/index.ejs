  <body>

   <!-- <div class="text-center">
      <div class="spinner" ng-hide="hideAppSpinner"></div>
    </div>
    <div  ng-show="appLoaded">-->
      <div >
    <header headroom offset='220' class="navbar-fixed-top">


      <nav class="navbar navbar-inverse navbar-opp" role="navigation" ng-controller="loginController"  ng-init="$parent.apiContext='<%= apiContext %>'">
        <div class="navbar-header">
          <ul class="nav navbar-nav pull-left visible-xs list-inline col-xs-9">
            <li ng-hide="loggedIn" class="col-xs-6"><a ng-click="openLoginPopup()" ng-hide="loggedIn">Login <strong class="caret"></strong></a></li>
            <li class="dropdown col-xs-6" ng-show="loggedIn">
              <a class="dropdown-toggle " href="#" data-toggle="dropdown" ng-cloak>{{state.user.firstName}} <strong class="caret"></strong></a>
              <ul class="dropdown-menu" style="padding: 15px; width:300px">

                <li><a href="#/myorder" class="link">My Order</a></li>
                <li><a href="" class="link" ng-click="logout()">Logout</a></li>

              </ul>
            </li>
            <li class="col-xs-6 "><a class="link cart pull-right" ng-click="checkout()"><i class="fa fa-shopping-cart fa-lg"></i>
                <span class="badge" ng-cloak="">{{cartImages? cartImages.length:0}} </span>
              </a>
            </li>
          </ul>

          <button type="button" class="navbar-toggle " data-toggle="collapse" data-target="#navbar-top-menu">
            <!-- <span class="glyphicon glyphicon-align-justify"> -->
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        </div>
        <div class="collapse navbar-collapse" id="navbar-top-menu">
          <ul class="nav navbar-nav">
            <li><a ui-sref="/">Home</a></li>
            <li><a ui-sref="pricing">Pricing</a></li>
            <li><a ui-sref="upload">Upload</a></li>
            <li><a ui-sref="who">Who We Are</a></li>
            <li><a ui-sref="contact">Contact US</a></li>

          </ul>
          <ul class="nav navbar-nav pull-right hidden-xs">
            <li ng-hide="loggedIn"><a ng-click="openLoginPopup()" ng-hide="loggedIn">Login <strong class="caret"></strong></a></li>
            <li class="dropdown" ng-show="loggedIn">
              <a class="dropdown-toggle " href="#" data-toggle="dropdown" ng-cloak>{{state.user.firstName}} <strong class="caret"></strong></a>
              <ul class="dropdown-menu" style="padding: 15px; width:300px">

                <li><a href="#/myorder" class="link">My Order</a></li>
                <li><a href="" class="link" ng-click="logout()">Logout</a></li>

              </ul>
            </li>
            <li><a class="link cart" ng-click="checkout()"><i class="fa fa-shopping-cart fa-lg"></i>
                <span class="badge" ng-cloak="">{{cartImages? cartImages.length:0}} </span>
              </a>
            </li>
          </ul>
        </div>

      </nav>
    </header>


    <div ui-view class="page-view"></div>
    </div>


